    <ExternalLibs>$(HOMEDRIVE)\otclient-msvc13-libs</ExternalLibs>
    <BoostVersion>1_55_0</BoostVersion>
    <BoostDir>$(ExternalLibs)\boost_$(BoostVersion)</BoostDir>
    <BoostLibs>$(BoostDir)\lib</BoostLibs>
    <BoostIncludes>$(BoostDir)\include</BoostIncludes>
