  <PropertyGroup>
    <IncludePath>$(WindowsSDK_v81);$(ProjectSources);$(ZLibIncludes);$(VorbisIncludes);$(PhysfsIncludes);$(OpenSSLIncludes);$(OpenALIncludes);$(OggIncludes);$(LuaIncludes);$(GlewIncludes);$(BoostIncludes);$(IncludePath)</IncludePath>
