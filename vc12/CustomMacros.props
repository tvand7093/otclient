    <OpenALLibs>$(OpenALDir)\lib</OpenALLibs>
    <OpenSSLVersion>1.0.1g</OpenSSLVersion>
    <OpenSSLDir>$(ExternalLibs)\OpenSSL-$(OpenSSLVersion)</OpenSSLDir>
    <OpenSSLLibs>$(OpenSSLDir)\lib\VC</OpenSSLLibs>
    <OpenSSLIncludes>$(OpenSSLDir)\include</OpenSSLIncludes>
    <PhysfsVersion>2.0.3</PhysfsVersion>
    <PhysfsDir>$(ExternalLibs)\physfs-$(PhysfsVersion)</PhysfsDir>
    <PhysfsIncludes>$(PhysfsDir)\include</PhysfsIncludes>
    <PhysfsLibs>$(PhysfsDir)\lib</PhysfsLibs>
    <ZLibVersion>1.2.8</ZLibVersion>
